#include<stdio.h>
#include<stdlib.h>
#include<math.h>

int main() {

int H,M,min,hr,d;
scanf("%d",&H);

scanf("%d",&M);

printf("Hours %d",H);
printf("Mins %d",M);

if((0<H<23) && (0<M<59))
{
if(M<=45)
{
d=45-M;
hr=H-1;
M=59;
min=M-d;
printf("min %d", min);
printf("hr %d",hr);
}
}

}
